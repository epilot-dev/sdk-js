  "devDependencies": {
    "@epilot/eslint-config": "^2.1.3",
    "@jest/types": "^30.0.0",
    "@types/node": "^24.0.1",
    "@types/yargs": "^17.0.33",
    "eslint": "^7.32.0",
    "lerna": "^6.4.1",
