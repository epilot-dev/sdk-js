{
  "name": "@epilot/validation-rules-client",
  "version": "1.1.9",
  "description": "API Client for epilot Validation Rules API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "bundle-definition": "webpack",
    "openapi": "node ../../scripts/update-openapi.js https://docs.api.epilot.io/validation-rules.yaml",
    "openapi:local": "node ../../scripts/update-openapi.js ../../../validation-rules-api/lambda/ApiHandlerFunction/openapi.yml",
    "typegen": "openapi typegen src/openapi.json --client -b '/* eslint-disable */' > src/openapi.d.ts",
    "build": "tsc && npm run build:patch && npm run bundle-definition",
    "build:patch": "sed -i'' -e '/^__exportStar.*openapi.*$/d' dist/index.js",
    "build:watch": "npm run build && tsc -w",
    "lint": "pnpm exec eslint src",
    "prepublishOnly": "npm run typegen && npm run build"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!node_modules",
    "!src",
    "!*.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/epilot-dev/sdk-js",
    "directory": "clients/validation-rules-client"
  },
  "keywords": [
    "validation",
    "rules",
    "epilot"
  ],
  "author": "epilot GmbH",
  "license": "MIT",
  "peerDependencies": {
    "axios": "^1.0.0 || >=0.25.0 <1.0.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "openapi-backend": "5.11.1",
    "openapi-client-axios": "^7.5.1",
    "stream-http": "^3.1.1",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "axios": "^1.11.0",
    "copy-webpack-plugin": "^7.0.0",
    "json-loader": "^0.5.7",
    "openapi-client-axios-typegen": "^7.5.1",
    "ts-loader": "^8.3.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}