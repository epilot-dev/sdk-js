{
    "name": "@epilot/query-client",
    "version": "1.5.0",
    "description": "API Client for epilot query-client API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "typescript": "tsc",
        "bundle-definition": "webpack",
        "openapi": "openapi read --json --dereference --bundle src/openapi.yml > src/openapi.json",
        "typegen": "echo '/* eslint-disable */' > src/openapi.d.ts && typegen src/openapi.yml >> src/openapi.d.ts",
        "build": "npm run openapi && npm run typegen && npm run typescript && npm run bundle-definition",
        "eslint": "../node_modules/.bin/eslint -c ../.eslintrc.yml"
    },
    "files": [
        "*.js",
        "*.d.ts",
        "**/*.js",
        "**/*.d.ts",
        "!*.test.*",
        "!**/*.test.*",
        "!node_modules",
        "!src",
        "!*.config.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/epilot-dev/sdk-js",
        "directory": "clients/query-client"
    },
    "keywords": [],
    "author": "epilot GmbH",
    "license": "UNLICENSED",
    "peerDependencies": {
        "axios": "^0.24.0",
        "@dazn/lambda-powertools-correlation-ids": "^1.28.1"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "https-browserify": "^1.0.0",
        "openapi-client-axios": "^4.4.9",
        "stream-http": "^3.1.1",
        "url": "^0.11.0",
        "util": "^0.12.3"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "axios": "^0.24.0",
        "copy-webpack-plugin": "^7.0.0",
        "jest": "^26.6.3",
        "json-loader": "^0.5.7",
        "openapi-client-axios-typegen": "^4.4.9",
        "openapicmd": "^1.8.2",
        "ts-jest": "^26.5.6",
        "ts-loader": "^8.3.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.5.4",
        "webpack": "^5.18.0",
        "webpack-cli": "^4.4.0"
    },
    "standard-version": {
        "skip": {
            "commit": true,
            "tag": true
        }
    }
}
