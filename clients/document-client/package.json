{
  "name": "@epilot/document-client",
  "version": "0.11.6",
  "description": "API Client for epilot document API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "bundle-definition": "webpack",
    "openapi": "node ../../scripts/update-openapi.js https://docs.api.epilot.io/document.yaml",
    "typegen": "echo '/* eslint-disable */' > src/openapi.d.ts && typegen ./src/openapi.json >> src/openapi.d.ts",
    "build": "tsc && npm run bundle-definition",
    "build:watch": "npm run build && tsc -w",
    "release-alpha": "npm version prerelease --preid alpha",
    "lint": "../../node_modules/eslint/bin/eslint.js src"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!node_modules",
    "!src",
    "!*.config.js"
  ],
  "keywords": [],
  "author": "epilot GmbH",
  "license": "UNLICENSED",
  "peerDependencies": {
    "axios": "^1.6.2"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "openapi-client-axios": "^7.5.1",
    "stream-http": "^3.1.1",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "axios": "^1.6.2",
    "copy-webpack-plugin": "^7.0.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "openapi-client-axios-typegen": "^7.4.1",
    "openapicmd": "^2.1.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  }
}
