{
  "name": "@epilot/entity-client",
  "version": "1.31.0",
  "description": "Client library for the epilot Entity API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "bundle-definition": "webpack",
    "openapi": "openapi read --json ../lambda/ApiHandlerFunction/openapi.yml > src/openapi.json",
    "typegen": "echo '/* eslint-disable */' > src/openapi.d.ts && typegen ../lambda/ApiHandlerFunction/openapi.yml >> src/openapi.d.ts",
    "build": "npm run openapi && npm run typegen && npm run typescript && npm run bundle-definition",
    "eslint": "../node_modules/.bin/eslint -c ../.eslintrc.yml",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.json",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!node_modules",
    "!src",
    "!src/**",
    "!*.config.js"
  ],
  "keywords": [],
  "author": "epilot GmbH",
  "license": "UNLICENSED",
  "peerDependencies": {
    "axios": "^0.21.1"
  },
  "dependencies": {
    "@dazn/lambda-powertools-correlation-ids": "^1.28.1",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "openapi-client-axios": "^4.4.9",
    "stream-http": "^3.1.1",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "axios": "^0.21.1",
    "copy-webpack-plugin": "^7.0.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "openapi-client-axios-typegen": "^4.4.9",
    "openapicmd": "^1.8.2",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  }
}
